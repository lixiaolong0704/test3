{
  "name": "moli.v2",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prestart": "npm run -s build",
    "start": "node dist/index.js",
    "dev": "nodemon  bin/www --exec \"node -r dotenv/config -r babel-register\"",
    "debug": "node  debug.js --exec \"node -r dotenv/config -r babel-register\"",
    "mondebug": "nodemon  debug.js --exec \"node -r dotenv/config -r babel-register\"",
    "clean": "rimraf dist",
    "build": "npm run clean && mkdir -p dist && babel src -s -D -d dist",
    "test": "jest --watch",
    "lint": "esw -w src test"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "body-parser": "~1.18.2",
    "co-express": "^2.0.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "ejs": "~2.5.7",
    "express": "~4.15.5",
    "lodash": "^4.17.4",
    "mobx-utils": "^3.2.2",
    "mongoose": "^5.0.0-rc0",
    "mongoose-paginate": "^5.0.3",
    "morgan": "~1.9.0",
    "node-sass-middleware": "0.9.8",
    "rimraf": "^2.6.2",
    "serve-favicon": "~2.4.5"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "babel-jest": "^21.2.0",
    "babel-register": "^6.26.0",
    "cfork": "^1.7.1",
    "dotenv": "^4.0.0",
    "eslint": "^4.12.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.3.2",
    "eslint-watch": "^3.1.3",
    "inspector-proxy": "^1.1.0",
    "jest": "^21.2.1",
    "mockjs": "^1.0.1-beta3",
    "nodemon": "^1.12.1",
    "supertest": "^3.0.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "import",
      "jest"
    ],
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "jest/no-focused-tests": 2,
      "jest/no-identical-title": 2
    }
  },
  "jest": {
    "testEnvironment": "node"
  }
}
